<!-- filtered list of upcoming pairings -->
<div class='list'>
  <h1>Available Pairing Sessions</h1>

    <%= render @available_pairings %>
  <% if current_user.phase == 2 || current_user.phase == 3 %>
    <%= link_to 'My Mentor Pairings', user_mentor_path(current_user), class: 'btn'  %><br><br>
  <% end %>
  <%= link_to 'My Student Pairings', user_student_path(current_user), class: 'btn' %>
</div>
